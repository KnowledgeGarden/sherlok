{
  "description" : "Example that annotates countries as an Country",
  "domain" : "examples",
  "language" : "en",
  "loadOnStartup" : false,
  "name" : "02.ruta.annotate.countries",
  "output" : {
    "include_annotations" : [ "org.sherlok.ruta.Country" ]
  },
  "script" : [ "WORDLIST CountriesList = 'countries.txt';", "DECLARE Country;", "Document{-> MARKFAST(Country, CountriesList)};" ],
  "tests" : [ {
    "in" : "Switzerland is close to Italy, the land of pasta and Michelangelo. Paris is in France. And Madrid in Spain.",
    "out" : {
      "1": {"@type" : "Country", "end" : 11 },
      "2": {"@type" : "Country", "begin" : 24,  "end" : 29 },
      "3": {"@type" : "Country", "begin" : 79,  "end" : 85 },
      "4": {"@type" : "Country", "begin" : 101,  "end" : 106 }
    }
  }, {
    "in" : "China's GDP has taken over the United States.",
    "out" : {
      "1": {"@type" : "Country", "end" : 5 },
      "2": {"@type" : "Country", "begin" : 31,  "end" : 44 }
    }
  } ],
  "version" : "1"
}
