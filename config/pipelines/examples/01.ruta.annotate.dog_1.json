{
  "name" : "01.ruta.annotate.dog",
  "version" : "1",
  "domain" : "examples",
  "description" : "Example that annotates 'dog' as an Animal",
  "engines" : [ {
    "script" : [
      "DECLARE Dog;",
      "W{REGEXP(\"dog\") -> MARK(Dog)};"
    ]
  } ],
  "output" : {
    "annotations" : [ "Dog" ]
  }
}
