{
  "name" : "01.ruta.annotate.dog",
  "version" : "1",
  "description" : "Example that annotates 'dog' as an Animal",
  "domain" : "examples",
  "script" : [
 "DECLARE Dog;"
, "W{REGEXP(\"dog\") -> MARK(Dog)};"
 ],
  "output" : {
    "include_annotations" : [ "org.sherlok.ruta.Dog" ]
  },
  "tests" : [ {
    "comparison" : "exact",
    "input" : "This is a very good dog!",
    "expected" : {
      "1" : { "begin" : 20, "end" : 23, "@type" : "Dog" }
    }
  } ]
}
