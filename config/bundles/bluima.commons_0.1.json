{
  "name": "bluima.commons",
  "domain": "bluima",
  "version": "1.0.1",
  "dependencies": [
    {
      "value": "ch.epfl.bbp.nlp:bluima_commons:1.0.1"
    }
  ],
  "config": {
    "bluima": {
      "type": "git", 
      "url": "https://github.com/BlueBrain/bluima_resources.git",
      "ref": "master"
    }
  },
  "engines": [
    {
      "name": "BlueBioLemmatizer",
      "class": "ch.epfl.bbp.uima.ae.BlueBioLemmatizer"
    },
    {
      "name": "LinnaeusAnnotator",
      "class": "ch.epfl.bbp.uima.ae.LinnaeusAnnotator",
      "parameters": {
        "CONFIG_FILE": [
          "$bluima/linnaeus/properties.conf"
        ]
      }
    }
  ]
}
